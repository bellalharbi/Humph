{
  "name": "Humph",
  "version": "1.1",
  "description": " ",
  "permissions": [
    "bookmarks",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Humph",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["scrape.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
